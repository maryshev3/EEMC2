<UserControl x:Class="EEMC2.Views.CourseWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EEMC2.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             xmlns:viewmodels="clr-namespace:EEMC2.ViewModels"
             d:DataContext="{d:DesignInstance Type=viewmodels:CourseVM}">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Button Content="Домой" Template="{DynamicResource RoundedButton}" Command="{Binding Path=ToHome}"/>
            <Label Content="{Binding Path=ObservableCourseFull.CourseName}"/>
        </StackPanel>
    </Grid>
</UserControl>
